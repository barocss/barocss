{
  "name": "@figmaikr/cssma-v3",
  "version": "0.0.1",
  "description": "Next-gen Tailwind CSS class parser (cssma-v3)",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "tailwindcss",
    "parser",
    "css",
    "utility",
    "tokenizer"
  ],
  "author": "FigmaikR",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.8.2",
    "vitest": "^1.6.1"
  }
}