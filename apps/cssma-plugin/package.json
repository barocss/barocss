{
  "name": "cssma-plugin",
  "version": "1.0.5",
  "description": "A powerful Figma plugin that enables seamless bidirectional conversion between Figma designs and CSS/TailwindCSS code",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm --filter cssma watch\" \"tsup --watch\"",
    "build": "pnpm --filter cssma build && tsup",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
