{
  "name": "cssma-react",
  "version": "2.0.0",
  "description": "React hooks and components for cssma",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "cssma": "workspace:*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.5.2",
    "typescript": "^5.7.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "figma",
    "tailwind",
    "css",
    "react",
    "hooks",
    "components"
  ],
  "author": "",
  "license": "MIT"
}